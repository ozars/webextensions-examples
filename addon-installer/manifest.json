{
  "browser_action": {
    "default_title": "Addon installer",
    "default_popup": "installer.html",
    "browser_style": true,
    "default_icon": {
        "128": "star-half.svg"
    }
  },
  "description": "An example of how to use the management API for addon installation.",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/addon-installer",
  "manifest_version": 2,
  "name": "Addon Installer",
  "permissions": [
    "management",
    "managementExtensions"
  ],
  "icons": {
      "128": "star-half.svg"
  },
  "version": "1.0",
  "applications": {
    "gecko": {
        "strict_min_version": "66.0.0"
    }
  }
}
